<Window x:Class="WpfAppRAP.CumulateCount"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfAppRAP"
        mc:Ignorable="d"
        Title="CumulateCount" Height="450" Width="800">
    <Grid>
        <DataGrid HorizontalAlignment="Left" Height="400" Margin="10,10,0,0" VerticalAlignment="Top" Width="392"
              ItemsSource="{Binding Source=MyList}"  AutoGenerateColumns="False">
            <DataGrid.Columns>
                <DataGridTextColumn Header="YEAR" Binding="{Binding ID}"/>
                <DataGridTextColumn Header="COUNT" Binding="{Binding Date}"/>
            </DataGrid.Columns>
        </DataGrid>

    </Grid>
</Window>
